<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
  logicalFilePath="changelog-4.1">

  <changeSet author="author" id="changelog-4.1">

    <dropNotNullConstraint tableName="tc_positions" columnName="course" columnDataType="FLOAT" />
    <dropNotNullConstraint tableName="tc_positions" columnName="speed" columnDataType="FLOAT" />
    <dropNotNullConstraint tableName="tc_positions" columnName="altitude" columnDataType="FLOAT" />
    <dropNotNullConstraint tableName="tc_positions" columnName="accuracy" columnDataType="DOUBLE" />
    <dropDefaultValue tableName="tc_positions" columnName="accuracy" />

  </changeSet>

</databaseChangeLog>
